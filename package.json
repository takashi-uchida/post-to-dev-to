{
  "name": "dev.to",
  "repository": {
    "type": "git",
    "url": "<Paste GitHub repository URL>"
  },
  "scripts": {
    "prettier": "prettier",
    "embedme": "embedme blog-posts/**/*.md",
    "prettier:base": "yarn run prettier blog-posts/\"**/*.{js,json,scss,md,ts,html,component.html}\"",
    "prettier:write": "yarn run prettier:base --write ",
    "prettier:check": "yarn run prettier:base --check",
    "embedme:check": "yarn run embedme --verify",
    "embedme:write": "yarn run embedme",
    "dev-to-git": "dev-to-git"
  },
  "dependencies": {
    "dev-to-git": "1.4.9",
    "embedme": "1.22.0",
    "prettier": "^2.6.2"
  }
}
